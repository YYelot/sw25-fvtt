          <div></div>
          <span style="text-align: right;"><br><b>{{localize "SW25.Item.Magicalsong.Cond"}}</b></span>
          <span class="grid-span-1" style="text-align: center;"><label for="{{item._id}}system.upcond"><b>{{localize "SW25.Item.Magicalsong.Up"}}</b></label><br>
